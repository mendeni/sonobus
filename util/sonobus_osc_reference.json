  "osc_mapping_reference": {
    "description": "Complete OSC address reference for SonoBus",
    "global_controls": [
      "/OutGainSlider - Output gain control (float, 0.0-1.0)",
      "/MainMuteButton - Main mute button (int, 0 or 1)",
      "/RecvSyncButton - Receive sync button (int, 0 or 1)",
      "/OptionsMaxRecvPaddingSlider - Maximum receive padding (float, 0.0-1.0)",
      "/OptionsRecStealth - Recording stealth mode (int, 0 or 1)"
    ],
    "input_groups": {
      "note": "Pattern applies to all groups 1-16. Replace N with group number (1-16)",
      "basic_controls": [
        "/InputGroupN/Mute - Mute toggle (int, 0 or 1)",
        "/InputGroupN/Solo - Solo toggle (int, 0 or 1)",
        "/InputGroupN/Gain - Input gain (float, 0.0-2.0)",
        "/InputGroupN/Pan - Pan position (float, -1.0 to 1.0, 0 is center)",
        "/InputGroupN/InvertPolarity - Polarity invert (int, 0 or 1)",
        "/InputGroupN/Monitor - Monitor level (float, 0.0-1.0)",
        "/InputGroupN/InReverbSend - Input reverb send (float, 0.0-1.0)",
        "/InputGroupN/MonReverbSend - Monitor reverb send (float, 0.0-1.0)"
      ],
      "compressor": [
        "/InputGroupN/Compressor/Enabled - Enable compressor (int, 0 or 1)",
        "/InputGroupN/Compressor/ThresholdDb - Threshold in dB (float, -60.0 to 0.0)",
        "/InputGroupN/Compressor/Ratio - Compression ratio (float, 1.0-20.0)",
        "/InputGroupN/Compressor/AttackMs - Attack time in ms (float, 0.1-100.0)",
        "/InputGroupN/Compressor/ReleaseMs - Release time in ms (float, 10.0-1000.0)",
        "/InputGroupN/Compressor/MakeupGainDb - Makeup gain in dB (float, 0.0-24.0)",
        "/InputGroupN/Compressor/AutomakeupGain - Auto makeup gain (int, 0 or 1)"
      ],
      "expander": [
        "/InputGroupN/Expander/Enabled - Enable expander/gate (int, 0 or 1)",
        "/InputGroupN/Expander/ThresholdDb - Threshold in dB (float, -60.0 to 0.0)",
        "/InputGroupN/Expander/Ratio - Expansion ratio (float, 1.0-20.0)",
        "/InputGroupN/Expander/AttackMs - Attack time in ms (float, 0.1-100.0)",
        "/InputGroupN/Expander/ReleaseMs - Release time in ms (float, 10.0-1000.0)",
        "/InputGroupN/Expander/MakeupGainDb - Makeup gain in dB (float, 0.0-24.0)",
        "/InputGroupN/Expander/AutomakeupGain - Auto makeup gain (int, 0 or 1)"
      ],
      "eq": [
        "/InputGroupN/EQ/Enabled - Enable EQ (int, 0 or 1)",
        "/InputGroupN/EQ/LowShelfGain - Low shelf gain in dB (float, -12.0 to 12.0)",
        "/InputGroupN/EQ/LowShelfFreq - Low shelf frequency in Hz (float, 20.0-500.0)",
        "/InputGroupN/EQ/Para1Gain - Parametric 1 gain in dB (float, -12.0 to 12.0)",
        "/InputGroupN/EQ/Para1Freq - Parametric 1 frequency in Hz (float, 20.0-20000.0)",
        "/InputGroupN/EQ/Para1Q - Parametric 1 Q factor (float, 0.1-10.0)",
        "/InputGroupN/EQ/Para2Gain - Parametric 2 gain in dB (float, -12.0 to 12.0)",
        "/InputGroupN/EQ/Para2Freq - Parametric 2 frequency in Hz (float, 20.0-20000.0)",
        "/InputGroupN/EQ/Para2Q - Parametric 2 Q factor (float, 0.1-10.0)",
        "/InputGroupN/EQ/HighShelfGain - High shelf gain in dB (float, -12.0 to 12.0)",
        "/InputGroupN/EQ/HighShelfFreq - High shelf frequency in Hz (float, 1000.0-20000.0)"
      ],
      "limiter": [
        "/InputGroupN/Limiter/Enabled - Enable limiter (int, 0 or 1)",
        "/InputGroupN/Limiter/ThresholdDb - Threshold in dB (float, -60.0 to 0.0)",
        "/InputGroupN/Limiter/Ratio - Limiting ratio (float, 10.0-100.0)",
        "/InputGroupN/Limiter/AttackMs - Attack time in ms (float, 0.01-10.0)",
        "/InputGroupN/Limiter/ReleaseMs - Release time in ms (float, 10.0-1000.0)"
      ],
      "monitor_delay": [
